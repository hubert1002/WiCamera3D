package com.wistron.StreamHelper;

/**
 * Exif Tag信息节点
 * @author WH1107028 Cocoonshu
 * @date 2012-02-03 16:35:56
 */
public class IFDInfo {
	
	/**
	 * sizeof(mType)*mCount <= 4 ? VALUE/OFFSET为这个TAG的值 : VALUE/OFFSET为这个TAG的值的偏移
	 */
	
	public int mTagName  = -1;   //Exif标签名称
	public int mType     = -1;   //Exif数据类型
	public int mCount    = 0;    //数据数量
	public int mOffset   = -1;    //数据偏移
	public Object mValue = null; //数据的值
}
